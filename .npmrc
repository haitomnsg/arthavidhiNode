# NPM configuration optimized for cPanel shared hosting
# Reduces memory usage and prevents resource exhaustion

# Limit concurrent network operations
maxsockets=3

# Disable progress bar (less CPU usage)
progress=false

# Use fewer audit calls
audit=false

# Don't fund messages
fund=false

# Reduce logging
loglevel=warn

# Prevent optional dependencies that may fail
optional=false

# Use legacy peer deps to avoid resolution issues
legacy-peer-deps=true
